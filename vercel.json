{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    }
  ],
  "env": {
    "MAX_IPS_PER_KEY": "3",
    "MAX_CONCURRENT_SESSIONS": "2",
    "SUSPICIOUS_DISTANCE_KM": "500",
    "IP_TRACKING_WINDOW": "3600"
  }
}
